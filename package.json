{
  "name": "es6-utils",
  "version": "1.2.0",
  "description": "provide some common utils like http request and so on,written by pure ES6",
  "main": "lib/index.js",
  "scripts": {
    "build:lib": "babel src --out-dir lib",
    "build:dist": "webpack --config webpack-build.config.js --progress --profile --colors",
    "dev": "json-mock-kuitos src/mock/db.json -s / -p 8080 & node server.js",
    "start": "json-mock-kuitos src/mock/db.json -s / -p 8080 & node server.js"
